{
    "name": "chadhutchins/oauth2-slack",
    "description": "Slack OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "slack",
        "slack api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Paterson",
            "email": "hello@adampaterson.co.uk"
        },
        {
            "name": "Chad Hutchins",
            "email": "hutchins.chad@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "1.*|2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~3.0",
        "phpunit/phpunit": "10"
    },
    "autoload": {
        "psr-4": {
            "Chadhutchins\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chadhutchins\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
